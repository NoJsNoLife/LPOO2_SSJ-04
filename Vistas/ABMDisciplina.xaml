        <Button Style="{StaticResource PrimaryButton}" x:Name="btnAgregarDisciplina" Content="Agregar Disciplina" HorizontalAlignment="Left" Margin="26,24,0,0" VerticalAlignment="Top" Height="31" Width="143" Click="btnAgregarDisciplina_Click"/>
        <Button Style="{StaticResource DangerButton}" x:Name="btnAtras" Content="Atrás" HorizontalAlignment="Left" Margin="634,23,0,0" VerticalAlignment="Top" Height="32" Width="146" Background="IndianRed" RenderTransformOrigin="0.928,0.758" Click="btnAtras_Click"/>
    </Grid>
